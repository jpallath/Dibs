<dl id="site" class="index">
<% user.forEach(function(user, i){ %>
<a class="indexlink" href="/users/<%=user._id%>">
	<dt><%= user.username %></dt>
	<dd><%= user.password %></dd>
	<br>
<form action= '/users/<%= user._id%>/?_method=DELETE' method="post">
  <input type= "submit" value = "Delete">
</a>
<% }); %>
</dl>
